<Window x:Class="OnlineShopOA1135.View.BasketWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnlineShopOA1135.View"
        mc:Ignorable="d"
        Title="BasketWin" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="Logo" FontSize="40"/>
        
        <ListBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Grid.RowSpan="5" ScrollViewer.VerticalScrollBarVisibility="Visible">
          
        </ListBox>
        
        <Button Grid.Column="1" Grid.Row="6" Content="delete" Background="White"/>
        <Button Grid.Column="3" Grid.Row="6" Content="заказать" Background="White"/>

        <Button Content="home" Grid.Row="1" Background="White" Command="{Binding UserMenuOpen}"/>
        <Button Content="prfl" Grid.Column="4" Background="White" Command="{Binding UserWinOpen}"/>
    </Grid>
</Window>
